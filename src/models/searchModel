const { ReadFileSync } = require('../helpers/fileHelpers');

var searchParams = {
  part: 'snippet',
  type: 'video',
  maxResults: 50, 
};


const searchArray = await ReadFileSync('search_filter');
const channelArray = await ReadFileSync('channel_filter');


module.exports = {
  searchParams,
  searchArray,
  channelArray,
};